using System;

namespace listesquÃªte1
{
    class Program
    {
        static void Main(string[] args)
        {
            System.Collections.Generic.List<char> liste = new System.Collections.Generic.List<char>();
            liste.Add('h');
            liste.Add('e');
            liste.Add('y');
            liste.RemoveAt(0);
            liste.Remove('y');
            liste[0] = 'a';

            foreach (char i in liste)
            {
                Console.WriteLine(i + " ");
            }


        }
    }
}
