using System;

class MainClass
{
    public static void Main(string[] args)
    {

        int[] tableauNum1 = new int[5] { 1, 2, 3, 4, 5 };

        int[] tableauNum2 = new int[5] { 2, 4, 6, 8, 10 };

        int[] tableauNum3 = new int[10];

        Array.Copy(tableauNum1, tableauNum3, 0);
        Array.ConstrainedCopy(tableauNum2, tableauNum3, 4);
    }
}
