using System;

class MainClass
{
    public static void Main(string[] args)
    {

        int[] tableauNum1 = new int[5] { 1, 2, 3, 4, 5 };

        int[] tableauNum2 = new int[5] { 6, 7, 8, 9, 10 };

        int[] tableauNum3 = new int[10];

        tableauNum1.CopyTo(tableauNum3, 0);
        tableauNum2.CopyTo(tableauNum3, 5);
    }
}
